        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.mapper.MemberMapper">
    <!--根据ID查询会员信息-->
    <select id="findById" parameterType="string" resultType="member">
